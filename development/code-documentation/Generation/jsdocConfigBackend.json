{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["./../../server/"],
    "includePattern": ".+\\.js$",
    "excludePattern": "(node_modules)"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "systemName": "Metis",
    "footer": "",
    "copyright": "Metis Backend (Version 1.0) - 2016 - Created by Kanbanana Backend Team",
    "navType": "vertical",
    "theme": "spacelab",
    "linenums": true
  },
  "opts": {
    "template": "node_modules/ink-docstrap/template",
    "encoding": "utf8",
    "destination": "../Documentation-Backend",
	"readme": "readmeBackend.md",
    "recurse": true,
    "private": true
  }
}