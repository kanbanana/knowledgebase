<queryTokenizer charArrayToken="" class="LetterOrDigitTokenizerFactory"/>
<filter class="LowerCaseFilter" scope="QUERY_INDEX"/>
<filter class="PolishStemFilter" minLength="3" scope="QUERY_INDEX"/>
